---
permalink: '/search-feed.json'
---
{
  "posts": [
    {% for item in collections.blog %}
      {
        "title": "{{ item.data.title }}",
        "url": "{{ item.url }}",
        "cat": "{{ item.data.cat }}",
        "cover": "{{ item.data.cover }}"
      }{% if not loop.last %},{% endif %}
    {% endfor %}
  ]
}
